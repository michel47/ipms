# pirl /.brings/system/bin/hip6.pl $*

set -e
url=https://raw.githubusercontent.com/Gradual-Quanta/minichain/master/.brings/minimal/system/bin/hip6.pl
qm=$(ipfs add -Q $url --progress=0)
qm=${qm:-QmVpfzYi8di41J1AzTBZSpXKGqNXytAAJQDXdSkxHeXc2M}
ipfs cat /ipfs/$qm | perl /dev/stdin $*
if false; then
 sed -i -e "s/qm=\${qm:-.*}$/qm=\${qm:-$qm}/" -e 's/if [t]rue;/if false;/' $0
fi
