sync_friend QmZV2jsMziXwrsZx5fJ6LFXDLCSyP7oUdfjXdHSLbLXxKJ michelc;
sync_friend QmVmTsmUVuphtck8AWsz4fhUwxy7187SzEPMbapcH52gvb emilea;

}

sync_friend()
{
   hash="$1"
   who="$2"

   if [ $debug -eq 1 ]; then
      echo "Entering in sunc_friend $*"
   fi

   if ! ipfs files stat --hash /my/friends 1>/dev/null 2>&1; then
      ipfs files mkdir -p /my/friends
   fi
   if ipfs name resolve ${hash} 1>/dev/null; then
      if ipfs files rm -r /my/friends/${who} 2>/dev/null; then true; fi
      if ipfs files cp /ipns/${hash} /my/friends/${who}; then true; fi
   else
      echo "warning: ${hash} not resolved"
   fi
}


main $@
exit $?; # $Source: /my/shell/scripts/syncnet.sh


